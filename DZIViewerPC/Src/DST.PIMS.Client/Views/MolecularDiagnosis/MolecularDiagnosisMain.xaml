<base:BaseUserControl x:Class="DST.PIMS.Client.Views.MolecularDiagnosisMain"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DST.PIMS.Client.Views"
             xmlns:base="clr-namespace:DST.Controls.Base;assembly=DST.Controls"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             mc:Ignorable="d" 
             d:DesignHeight="940" d:DesignWidth="1920">
    <Grid>
        <hc:TabControl x:Name="TabControlDemo"
                       IsAnimationEnabled="True"
                       IsDraggable="True"
                       Background="White">
            <hc:TabItem Header="PCR"
                        IsSelected="True"
                        hc:IconElement.Height="16"
                        hc:IconElement.Width="16"
                        hc:IconElement.Geometry="{StaticResource DocumentGeometry}">
                <local:MoleDiagExamed />
            </hc:TabItem>
        </hc:TabControl>
    </Grid>
</base:BaseUserControl>
